{
  "manifest_version": 2,

  "name": "vk-x",
  "description": "__MSG_extension_description__",
  "default_locale": "ru",
  "version": "1.6.0",

  "key": "",

  "icons": {
    "48": "icons/vk-x-48.png",
    "128": "icons/vk-x-128.png"
  },

  "permissions": [
    "storage",
    "https://api.vk.com/"
  ],

  "content_scripts": [{
    "run_at": "document_start",
    "matches": ["https://vk.com/*"],
    "js": ["content-script.js"]
  }],

  "web_accessible_resources": ["injected.js"]
}
